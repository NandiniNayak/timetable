<%= simple_form_for(@time_table) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :week , collection: ["a","b"] %>
    <%= f.input :date %>
    <%= f.input :day %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :period %>
    <%= f.input :room_number %>
    <%= f.input :subject %>
    <%= f.input :teacher %>
    <%= f.input :equipment ,collection: ["TAS uniform","sports uniform"] %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
